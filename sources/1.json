{
    "name": "宜搜小说-星月",
    "url": "ieasou.com",
    "version": 100,
    "search": {
        "url": "http://api.ieasou.com/api/bookapp/searchdzh.m?word=${key}&type=0&page_id=1&count=20&cid=eef_easou_book&os=android&appverion=1115",
        "charset": "UTF-8",
        "list": "$.all_book_items[*]",
        "name": "$.name",
        "author": "$.author",
        "cover": "$.imgUrl",
        "detail": "http://api.ieasou.com/api/bookapp/bookSummary.m?nid=${$.nid}&gid=${$.gid}&returnType=010&cid=eef_easou_book&os=android&appverion=1115"
    },
    "detail": {
        "summary": "${$..summary}    ${$..wordCount}\n${$..labels}\n${$..desc}@replace->,-> ",
        "category": "$..labels@replace->,-> ",
        "status": "$..status@equal->1@replace->true->连载",
        "words": "$..wordCount",
        "update": "$..last_time",
        "lastChapter": "$..last_chapter_name",
        "catalog": "http://api.ieasou.com/api/bookapp/bookSummary.m?gid=${$.coverInfo.gid}&nid=${$.coverInfo.nid}&sort=1&size=100000&returnType=100&cid=eef_easou_book&os=android&appverion=1115"
    },
    "catalog": {
        "list": "$.volumes[*]",
        "booklet": {
            "name": "$.name",
            "list": "$.chapters[*]"
        },
        "name": "$.chapter_name",
        "chapter": "http://api.ieasou.com/api/bookapp/chargeChapter.m?cid=eef_easou_book&os=android&udid=11db225c8bc8fd33cb6b16507eb2966b672b228e&ch=blf1298_10928_001&appverion=1115&gid=${$params.gid}&nid=${$.nid}&sort=${$.sort}"
    },
    "chapter": {
        "content": "$.content"
    }
}