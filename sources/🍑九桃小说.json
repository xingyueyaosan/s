{
    "name": "🍑九桃小说",
    "url": "www.9taoxs.com",
    "version": 100,
    "search": {
        "url": "https://www.9taoxs.com/search.html@post->searchkey=${key}",
        "charset": "UTF-8",
        "list": "ul.library > li",
        "name": "a.bookname",
        "author": "a.author",
        "cover": "a.booking > img",
        "detail": "a.bookname"
    },
    "detail": {
        "summary": "meta[name=description]@attr->content",
        "status": "meta[property=og:novel:status]@attr->content",
        "update": "meta[property=og:novel:update_time]@attr->content",
        "catalog": "p.action > a:nth-child(1)"
    },
    "catalog": {
        "list": ".read > dl:gt(0)",
        "booklet": {
            "name": "b@match->(?<= )(.+)",
            "list": "dd"
        },
        "name": "a",
        "chapter": "a"
    },
    "chapter": {
        "content": "#content",
        "purify": [
            "九桃小说(.+)\\n|(.+)九桃(.+)节！"
        ]
    }
}
