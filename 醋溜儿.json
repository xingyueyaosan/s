        "status": "[property=og:novel:status]@attr->content",
        "update": "[property=og:novel:update_time]@attr->content",
        "catalog": ".bfirst"
    },
    "catalog": {
        "list": "li",
        "page": ".location>div>a:nth-last-child(2)",
        "name": "span",
        "chapter": "a"
    },
    "chapter": {
        "content": ".content"
    }
}
