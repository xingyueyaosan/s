{
    "name": "笔趣阁-星",
    "url": "biqugx.com",
    "version": 100,
    "search": {
        "url": "https://www.biuqge.com/modules/article/search.php?searchtype=all&searchkey=${key}&t_btnsearch=",
        "charset": "utf-8",
        "list": ".c_row",
        "name": "span > a",
        "author": ".c_value",
        "cover": "img",
        "detail": "a"
    },
    "detail": {
        "summary": "${td:contains(分类)}    ${td:contains(字数)}\n${.tabvalue}",
        "status": "meta[property=og:novel:status]@attr->content",
        "update": "meta[property=og:novel:update_time]@attr->content",
        "catalog": "a:contains(点击阅读)"
    },
    "catalog": {
        "list": ".index > dd",
        "name": "a",
        "chapter": "a"
    },
    "chapter": {
        "content": ".acontent"
    }
}