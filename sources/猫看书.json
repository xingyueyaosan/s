{
    "name": "猫看书-星",
    "url": "m.maokanshu.com",
    "version": 100,
    "search": {
        "url": "https://www.maokanshu.com/search.html@post->s=articlename&searchkey=${key}",  
        "charset": "utf-8",
        "list": "#alistbox",
        "name": "h2>a",
        "author": "span@replace->作者：->",
        "cover": "img",
        "detail": "a"
    },
    "detail": {
        "summary": "${td:(分类)}  ${td:(字数)}\n${.intro}",
        "status": ".infotop@match->(?<=状态：).+(?=点)",
        "update": "td:matches(时间):not(b)",
         "catalog": ".btopt>a"
    },
    "catalog": {
        "list": "#list>dl>dd",
        "name": "a",
        "chapter": "a"
    },
    "chapter": {
        "content":"#content>p",
        "purify": [
          "(.+)m.maokanshu.com(.+)"
        ]
    }
}