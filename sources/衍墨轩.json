{
    "name": "衍墨轩-星",
    "url": "ymoxuan.org",
    "version": 100,
    "search": {
        "url": "http://www.ymoxuan.org/search.htm?keyword=${key}",
        "charset": "utf-8",
        "list": ".lastest>ul>li",
        "name": ".n2>a",
        "author": ".a2>a",
        "detail": ".n2>a"
    },
    "detail": {
        "cover": ".cover>img",
        "summary": "${.pt20>i:eq(1)}\n${.desc:not(b)}",
        "status": ".pt20>i:eq(2)",
        "update": "p:matches(更新时间)>i",
        "catalog": "a:matches(目录)"
    },
    "catalog": {
        "list": ".mulu>li:nth-child(n+11)",
        "name": "li",
        "chapter": "a"
    },
    "chapter": {
        "content": ".content",
        "purify": [
            "(【≮|≮).+((≯)|(≯.+】.+?)?)",
            "%? .+yanmoxuan.+"
        ]
    }
}