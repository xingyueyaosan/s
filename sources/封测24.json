{
    "name": "速阅阁_24",
    "url": "suyueg.com",
    "version": 100,
    "search": {
        "url": "https://m.suyueg.com/book/search.html?q=${key}",
        "charset": "gbk",
        "list": ".searchcon > ul",
        "name": "h3",
        "author": "h3>a@attr->href",
        "detail": "https://img.suyueg.com/${h3>a@attr->href}xyys${h3>a@attr->href}${h3>a@attr->href}s.jpg@replace->xyys/book/->",
        "cover": "${h3>a@attr->href}D@match->(\\d+).+@replace->D->B"
    },
    "detail": {
        "cover": "",
        "summary": "${span:matches(类别)>a}\n${p.jj}",
        "status": "meta[property=og:novel:status]@attr->content",
        "update": "meta[property=og:novel:update_time]@attr->content",
        "catalog": ""
    },
    "catalog": {
        "list": "li.chapter",
        "name": "a",
        "chapter": "a"
    },
    "chapter": {
        "content": "#articlecontent",
        "purify": [
            "还在(.+)\n(.+)百度(.+)\n(.+)速阅阁\\)"
        ]
    }
}