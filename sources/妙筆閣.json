{
    "name": "妙筆閣-星月",
    "url": "imiaobige.com",
    "version": 100,
    "search": {
        "url": "https://www.imiaobige.com/search.html@post->searchkey=${key}",
        "charset": "UTF-8",
        "list": "#sitembox > dl",
        "name": "h3 > a",
        "author": "dd.book_other > a:nth-child(2)",
        "cover": "a > img",
        "detail": "h3 > a"
    },
    "detail": {
        "summary": "meta[property=og:description]@attr->content",
        "status": "meta[property=og:novel:status]@attr->content",
        "update": "meta[property=og:novel:update_time]@attr->content",
        "catalog": ".motion > a:matches(开始阅读)"
    },
    "catalog": {
        "list": "#readerlists > ul:gt(0)",
        "booklet": {
            "name": "h3",
            "list": "li"
        },
        "name": "a",
        "chapter": "a"
    },
    "chapter": {
        "content": "#content",
        "purify": [
            ".+度.+节！",
            "妙笔阁(.+)\\n"
        ]
    }
}
