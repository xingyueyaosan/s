{
    "name": "速阅阁+",
    "url": "suyueg.com",
    "version": 100,
    "search": {
        "url": "https://m.suyueg.com/book/search.html?q=${key}",
        "charset": "gbk",
        "list": ".searchcon > ul",
        "name": "h3",
        "author": "li + li@match->(?<=：)(.+)",
        "detail": "https://img.suyueg.com/${h3>a}xyys${h3>a}${h3>a}s.jpg@match->(^[book])",
        "cover": "https://www.suyueg.com${h3>a}@replace->book->0"
    },
    "detail": {
        "cover": ".pic > img",
        "summary": "${span:matches(类别)>a}\n${p.jj}",
        "status": "meta[property=og:novel:status]@attr->content",
        "update": "meta[property=og:novel:update_time]@attr->content",
        "catalog": ""
    },
    "catalog": {
        "list": ".chapter",
        "name": "a",
        "chapter": "a"
    },
    "chapter": {
        "content": "#articlecontent",
        "purify": [
            "还在(.+)\n(.+)百度(.+)\n(.+)速阅阁\\)"
        ]
    }
}
