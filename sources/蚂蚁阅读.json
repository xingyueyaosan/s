{
    "name": "蚂蚁阅读-星月",
    "url": "imayitxt.com",
    "version": 100,
    "search": {
        "url": "http://www.imayitxt.com/modules/article/search.php?q=${key}",
        "charset": "utf-8",
        "list": "tbody > tr:gt(0)",
        "name": "td:nth-child(3)",
        "author": "td:nth-child(6)",
        "detail": "td:nth-child(3) > a"
    },
    "detail": {
        "words": "span:matches(总字数：)",
        "cover": ".img > img",
        "summary": ".brief.breviary",
        "update": ".list > a",
        "lastChapter": ".list > i",
        "catalog": "a:matches(目录)"
    },
    "catalog": {
        "list": ".float-list.fill-block > li",
        "name": "a",
        "chapter": "a"
    },
    "chapter": {
        "content": "#ChapterContents",
        "purify": [
            " 一秒记住♂ ，更新快，，免费读！"
        ]
    }
}