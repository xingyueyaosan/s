{
    "name": "猫看书-星",
    "url": "maokanshu.com",
    "version": 100,
    "search": {
        "url": "https://m.maokanshu.com/search.html@post->searchkey=${key}",
        "charset": "utf-8",
        "list": "ul.list > li",
        "name": "p.bookname",
        "author": "p.author@replace->@span->",
        "cover": "img",
        "detail": "a"
    },
    "detail": {
        "summary": "${a.layui-btn}    ${span.layui-bg-red}\n${.intro}@replace-> 万->万",
        "status": "meta[property=og:novel:status]@attr->content",
        "update": "meta[property=og:novel:update_time]@attr->content",
        "catalog": ".action > li > a"
    },
    "catalog": {
        "list": "ul.read > li",
        "page": "select>option[selected]+option@attr->value",
        "name": "a",
        "chapter": "a"
    },
    "chapter": {
        "content": ".content > p"
    }
}
